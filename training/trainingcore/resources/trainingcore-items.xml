<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
        <collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
        <collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true"
                        type="set"/>
    </collectiontypes>

    <enumtypes>
        <enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
            <value code="BLACK"/>
            <value code="BLUE"/>
            <value code="BROWN"/>
            <value code="GREEN"/>
            <value code="GREY"/>
            <value code="ORANGE"/>
            <value code="PINK"/>
            <value code="PURPLE"/>
            <value code="RED"/>
            <value code="SILVER"/>
            <value code="WHITE"/>
            <value code="YELLOW"/>
        </enumtype>
    </enumtypes>
    <relations>

        <relation code="Dealer2Vehicle" autocreate="true" localized="false">
            <deployment table="Dealer2Vehicle" typecode="12350"/>
            <sourceElement qualifier="dealers" type="Dealer" cardinality="many" ordered="false">
                <description>Dealers</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="vehicles" type="Vehicle" cardinality="many" collectiontype="set">
                <description>Vehicles</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

        <relation code="Dealer2Driver" autocreate="true" localized="false">
            <deployment table="Dealer2Driver" typecode="12351"/>
            <sourceElement qualifier="dealers" type="Dealer" cardinality="many" ordered="false">
                <description>Dealers</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="drivers" type="Driver" cardinality="many" collectiontype="set">
                <description>Drivers</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

        <relation code="Driver2Vehicle" autocreate="true" localized="false">
            <deployment table="Driver2Vehicle" typecode="12352"/>
            <sourceElement qualifier="drivers" type="Driver" cardinality="many" ordered="false">
                <description>Drivers</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="vehicles" type="Vehicle" cardinality="many" collectiontype="set">
                <description>Vehicles</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

        <relation code="Vehicle2Accessories" autocreate="true" localized="false">
            <deployment table="Vehicle2Accessories" typecode="12361"/>
            <sourceElement qualifier="vehicles" type="SupportedVehicle" cardinality="many" ordered="false">
                <description>Accessory Supported Vehicle</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="vehicleAccessoryStyleVariant" type="VehicleAccessoryStyleVariantProduct" cardinality="many" collectiontype="set">
                <description>Vehicles</description>
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

    </relations>

    <itemtypes>

        <!-- Add your item definitions here -->


        <!-- TypeGroups are for structure of this file only -->

        <typegroup name="Apparel">
            <itemtype code="ApparelProduct" extends="Product"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelProduct">
                <description>Base apparel product extension that contains additional attributes.</description>
                <attributes>
                    <attribute qualifier="genders" type="GenderList">
                        <description>List of genders that the ApparelProduct is designed for</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelStyleVariantProduct">
                <description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="style" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color/Pattern of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="swatchColors" type="SwatchColorSet">
                        <description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>

            </itemtype>

            <itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ApparelSizeVariantProduct">
                <description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
                <attributes>
                    <attribute qualifier="size" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Size of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>





        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="de.hybris.training.core.jalo.ElectronicsColorVariantProduct">
                <description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="TrainingEmployee"
                      extends="Employee"
                      autocreate="true"
                      generate="true">
                <attributes>
                    <attribute qualifier="employeeName" type="localized:java.lang.String" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                    </attribute>
                    <attribute qualifier="employeeAge" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="Vehicle"
                      autocreate="true"
                      generate="true"
            extends="Product">
                <attributes>
                    <attribute qualifier="registrationNo" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="Dealer"
                      autocreate="true"
                      generate="true">
                <deployment table="Dealer" typecode="12346"/>
                <attributes>
                    <attribute qualifier="dealerId" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" initial="true" unique="true"/>
                    </attribute>
                    <attribute qualifier="name" type="localized:java.lang.String" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                    </attribute>
                    <attribute qualifier="address" type="Address" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="Driver"
                      autocreate="true"
                      generate="true">
                <deployment table="Driver" typecode="12347"/>
                <attributes>
                    <attribute qualifier="driverId" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" initial="true" unique="true"/>
                    </attribute>
                    <attribute qualifier="name" type="localized:java.lang.String" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                    </attribute>
                    <attribute qualifier="experience" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <modifiers read="true" write="true" optional="true"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="VehicleAccessory">


            <itemtype code="VehicleAccessoryProduct" extends="Product"
                      autocreate="true" generate="true">
                <description>Base Vehicle Accessory product extension that contains additional attributes.</description>
                <attributes>
                    <attribute qualifier="accessoryName" type="localized:java.lang.String" generate="true">
                        <description>Name of the Vehicle Accessory</description>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="accessoryBrandName" type="localized:java.lang.String" generate="true">
                        <description>Brand name of the Vehicle Accessory</description>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="VehicleAccessoryStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true">
                <description>Vehicle Accessory style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="accessoryColor" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="SupportedVehicle"
                      autocreate="true"
                      generate="true">
                <description>Vehicle that supports accessories</description>
                <deployment table="SupportedVehicle" typecode="12360"/>
                <attributes>
                    <attribute qualifier="code" type="java.lang.Integer" generate="true">
                        <persistence type="property"/>
                        <description>Unique Identifier code of the vehicle.</description>
                        <modifiers read="true" write="true" optional="false" initial="true" unique="true"/>
                    </attribute>
                    <attribute qualifier="vehicleName" type="localized:java.lang.String" generate="true">
                        <persistence type="property"/>
                        <description>Name of the Vehicle.</description>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                    </attribute>
                    <attribute qualifier="variant" type="localized:java.lang.String" generate="true">
                        <persistence type="property"/>
                        <description>Name of the Vehicle.</description>
                        <modifiers read="true" write="true" optional="false" initial="true"/>
                    </attribute>
                </attributes>
            </itemtype>

        </typegroup>

    </itemtypes>
</items>
